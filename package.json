{
  "name": "dynamo-table",
  "version": "0.6.3",
  "description": "Maps JS objects and queries to DynamoDB tables",
  "main": "index.js",
  "scripts": {
    "test": "mocha -t 100s",
    "coverage": "mocha --require blanket -R html-cov > coverage.html",
    "istanbul": "istanbul cover ./node_modules/.bin/_mocha",
    "blanket": {
      "pattern": "dynamo-table/index.js",
      "data-cover-flags": {
        "branchTracking": true
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mhart/dynamo-table.git"
  },
  "keywords": [
    "dynamo",
    "dynamodb",
    "mapping"
  ],
  "author": "Michael Hart <michael.hart.au@gmail.com> (http://github.com/mhart)",
  "license": "MIT",
  "devDependencies": {
    "should": "~1.2.2",
    "mocha": "~1.9.0",
    "async": "~0.2.7",
    "fake-dynamo-gem": "~0.0.3"
  },
  "optionalDependencies": {
    "dynamo-client": "~1.0.0"
  }
}
